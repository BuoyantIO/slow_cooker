{
  "id": "/slow-cooker",
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "buoyantio/slow_cooker:latest",
      "forcePullImage": false,
      "network": "BRIDGE"
    }
  },
  "cmd": "/go/bin/slow_cooker -qps 100 -concurrency 10 -url http://$HOST:4140",
  "cpus": 1.0,
  "mem": 256.0,
  "uris": [
    "file:///etc/docker.tar.gz"
  ]
}
